{
	"manifest_version": 2,

	"name":"Alexa Search",
	"version":"1.0",

	"description" : "Search on diferent way with Amazon Alexa",
	"browser_action":{
		"default_title" : "Hi, Alexa!",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
   		"persistent": false,
    	"scripts": ["background.js"]
  	},
	"permissions":[
		"http://*/*", 
		"http://*/*", 
		"tabs",
		"background" 
	]
}