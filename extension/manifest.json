{
	"manifest_version": 2,

	"name":"Alexa Search",
	"version":"1.0",

	"description" : "Search with Amazon Alexa",
	"browser_action":{
		"default_title" : "Hi, Alexa!",
		"default_icon": "icon.png",
		"default_popup":"popup.html"
	},
	"permissions":[
		"tabs",
		"storage",
		"tabs",
		"https://www.google.com/_/chrome/newtab*"
	]
}
